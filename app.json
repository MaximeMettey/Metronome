{
  "expo": {
    "name": "Metronome Pro",
    "slug": "metronome-pro",
    "version": "2.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.metronomepro.app",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "com.metronomepro.app",
      "permissions": ["VIBRATE"]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
